	.inesMap 0
	.inesPrg "32kb"
	.inesChr "8kb"
	.inesMir 0

	.romSegment "32kb","32kb","PRG"
	.bank 0
	.org $8000

	LDA #5 + 2

	.repeat 3, \j
		.db 5 + \j
		.repeat 4, \k
		LDX #\k
			.repeat 5, \l
			LDX #\l
			.endRepeat
		.endRepeat
	.endRepeat

	.repeat 2
	ldx #6
	.endRepeat

	.romSegment "8kb","8kb","CHR"
	.bank 0
	.org $0000
